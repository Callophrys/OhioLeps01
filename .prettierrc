{
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"printWidth": 400,
	"singleQuote": true,
	"indentStyle": "space",
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": false,
	"plugins": [
		"prettier-plugin-svelte"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}